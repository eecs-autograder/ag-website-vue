{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.config.json"
    },
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.cypress-ct.json"
    }
  ],
  "target": "es6",
  "compilerOptions": {
    "downlevelIteration": true,
    "module": "esnext",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": [
      "node",
      "jest"
    ],
    "typeRoots": [
      "./types",
      "node_modules/@types"
    ],
    "paths": {
      "@/*": [
          "src/*"
      ],
      "@/tests/*": [
          "tests/*"
      ],
      "highlight.js": [
          "src/redefined_types/highlightjs.d.ts"
      ]
    },
    "lib": [
      "es2017",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
    "./types/**/*.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "tests/**/*.ts",
    "tests/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
